{
  "name": "shopflex-site-ssr",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "templates/*",
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn install",
    "prepare": "husky install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,vue,json}\""
  },
  "vetur": {
    "tags": "node_modules/element-helper-json/element-tags.json",
    "attributes": "node_modules/element-helper-json/element-attributes.json"
  },
  "lint-staged": {
    "*.{js,vue,md,css,scss,ts}": "yarn format"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.3",
    "cross-env": "^7.0.3",
    "element-helper-json": "^2.0.6",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.11"
}